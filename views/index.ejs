        <script>
          window.sr = ScrollReveal();
          sr.reveal('.navbar', {
            duration: 2000,
            origin: 'bottom'
          });
          sr.reveal('.info-left', {
            duration: 2000,
            origin: 'left',
            distance: '200px'
          });
          sr.reveal('.info-right', {
            duration: 2000,
            origin: 'right',
            distance: '200px'
          });
          sr.reveal('.under', {
            duration: 2000,
            origin: 'bottom',
            distance: '100px',
            viewFactor: 0.2,
          });

          sr.reveal('.jumbotron', {
              duration: 2000,
              origin: 'bottom',
              distance: '100px',

          });
