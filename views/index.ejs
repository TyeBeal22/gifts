<!--contact us-->

<div class="wrapper" id="contacts">
  <div class="company-info">
    <h3 class="poppin">Grinder Gifts</h3>
    <ul>
      <li><i class="fa fa-road"></i> Arlington, VA</li>
      <li><i class="fa fa-phone"></i> 571-277-7641</li>
      <li><i class="fa fa-envelope"></i> admin@grindergifts.com</li>
    </ul>
  </div>
  <div class="contact">
    <h3 class-"poppin">Contact Us</h3>

    <div class="alert"> Message sent! </div>
    <form id="contactForm">
      <p>
        <label>Name</label>
        <input type="text" name="name" id="name" required>
      </p>
      <p>
        <label>Company</label>
        <input type="text" name="company" id="company">
      </p>
      <p>
        <label>Email Address</label>
        <input type="email" name="email" id="email" required>
      </p>
      <p>
        <label>Phone Number</label>
        <input type="text" name="phone" id="phone">
      </p>
      <p class="full">
        <label>Message</label>
        <textarea name="message" rows="8" id="message"></textarea>
      </p>
      <p class="full">
        <button type="submit">Submit</button>
      </p>
    </form>
  </div>
</div>

